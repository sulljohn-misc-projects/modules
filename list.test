#!/bin/bash

# suppress pushd and popd output                                               
pushd () {                                                                     
    command pushd "$@" > /dev/null                                             
}                                                                            
popd () {                                                                     
		command popd "$@" > /dev/null                                               
}

let X=1
pushd .
while [ ${X} -lt 10 ] ; do
		if [ ${X} -lt 4 ] ; then
				runtest_fail.sh 0 "test${X}"
		else
				runtest_fail.sh 1 "test${X}"
		fi
		let X=X+1
done
popd
